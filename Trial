<html>
<head>
<script>
function fun()
{
var i,sum=0,n;
n=parseInt(document.getElementById("number").value);
for(i=1;i<=(n/2);i++)
if(n%i==0)
sum=sum+i;
if(sum==n)
alert(n+" is Perfect");
else if(sum<n)
alert(n+" is Deficient");
else
alert(n+" is abundant");
}
</script>
</head>
<body>
<input type="text" placeholder="Enter a number" id="number">
<input type="submit" value="Submit" onClick="fun()">
</body>
</html>
